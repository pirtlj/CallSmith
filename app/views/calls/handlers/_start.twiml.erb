<?xml version="1.0" encoding="UTF-8"?>
<Response>

	<%if current_user.calls.today.count > 0%>
  		<Say voice="woman">Calling <%= current_user.calls.today.count%> numbers</Say>
		<Redirect><% dial_call_url(current_user.calls.today.first, :format => :twiml, :auth_token => current_user.authentication_token)%></Redirect>
	<%else%>
	  	<Say voice="woman">You have no calls scheduled</Say>
	<%end%>
</Response>