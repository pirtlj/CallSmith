

<%= semantic_form_for @call do |f| %>
    
 	<%= f.inputs do %>
    	<%= f.input :contact_id, :as => :select,  :collection => current_user.contacts.map{|contact| ["#{contact.first_name} #{contact.last_name}", contact.id]}%>
		<%= f.input :scheduled_for, :as => :datepicker%>
	<% end %>
    <%= f.buttons %>
  <% end %>